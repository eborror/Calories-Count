<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="calories_count.files.PrimaryController">
   <children>
      <Label text="Calories Count!" style="-fx-font-size: 24px; -fx-font-weight: bold; -fx-padding: 0 0 20 0;" />
      <Button text="Go to User Form" onAction="#switchToUserForm" />
      <Button text="Lookup food items" onAction="#switchToFoodLookup" />
      <Button text="View Today's Logged Foods" onAction="#openTrackedFoods" />
      <Button text="View Today's Macro Chart" onAction="#switchToMacroChart" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>